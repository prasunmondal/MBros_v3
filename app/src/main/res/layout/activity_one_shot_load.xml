<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background2"
    tools:context=".OneShot.Delivery.OneShotLoad">

    <LinearLayout
        android:id="@+id/linearLayout23"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="#000000"
        android:orientation="vertical"
        android:paddingHorizontal="10dp"
        android:paddingVertical="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView61"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/biorhyme"
            android:text="Mondal Bros."
            android:textColor="#FBF0D6"
            android:textSize="20sp"
            android:textStyle="bold" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout23">

        <LinearLayout
            android:id="@+id/osl_company_details_outer_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="5dp"
            android:background="@drawable/rounded"
            android:backgroundTint="#9E232323"
            android:orientation="vertical"
            android:paddingHorizontal="15dp"
            android:paddingTop="5dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView45"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/bai_jamjuree_medium"
                    android:text="Company Details"
                    android:textColor="#C998958E"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <View
                    android:id="@+id/view"
                    android:layout_width="wrap_content"
                    android:layout_height="1dp"
                    android:layout_weight="1" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingTop="10dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/textView26"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/bai_jamjuree_medium"
                                    android:text="Company"
                                    android:textColor="@color/osl_ui_attribute_label_text_color"
                                    android:textSize="13sp" />

                                <AutoCompleteTextView
                                    android:id="@+id/osl_label_company_name_2"
                                    android:layout_width="match_parent"
                                    android:layout_height="35dp"
                                    android:layout_weight="1"
                                    android:completionThreshold="0"
                                    android:imeOptions="actionNext"
                                    android:inputType="textNoSuggestions"
                                    android:textColor="@color/osl_ui_attribute_value_text_color"
                                    android:textSize="14sp"
                                    tools:ignore="TouchTargetSizeCheck" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/textView54"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/bai_jamjuree_medium"
                                    android:text="Branch"
                                    android:textColor="@color/osl_ui_attribute_label_text_color"
                                    android:textSize="13sp" />

                                <AutoCompleteTextView
                                    android:id="@+id/osl_label_branch_name_2"
                                    android:layout_width="match_parent"
                                    android:layout_height="35dp"
                                    android:layout_weight="1"
                                    android:completionThreshold="0"
                                    android:imeOptions="actionNext"
                                    android:inputType="textNoSuggestions"
                                    android:textColor="@color/osl_ui_attribute_value_text_color"
                                    android:textSize="14sp"
                                    tools:ignore="TouchTargetSizeCheck" />
                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="horizontal"
                            android:paddingTop="10dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/textView55"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/bai_jamjuree_medium"
                                    android:text="Area"
                                    android:textColor="@color/osl_ui_attribute_label_text_color"
                                    android:textSize="13sp" />

                                <AutoCompleteTextView
                                    android:id="@+id/osl_label_area_name_2"
                                    android:layout_width="match_parent"
                                    android:layout_height="35dp"
                                    android:layout_weight="1"
                                    android:completionThreshold="0"
                                    android:imeOptions="actionNext"
                                    android:inputType="textNoSuggestions"
                                    android:textColor="@color/osl_ui_attribute_value_text_color"
                                    android:textSize="14sp"
                                    tools:ignore="TouchTargetSizeCheck" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/textView57"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:fontFamily="@font/bai_jamjuree_medium"
                                    android:text="Account"
                                    android:textColor="@color/osl_ui_attribute_label_text_color"
                                    android:textSize="13sp" />

                                <AutoCompleteTextView
                                    android:id="@+id/osl_label_money_account_name_2"
                                    android:layout_width="match_parent"
                                    android:layout_height="35dp"
                                    android:layout_weight="1"
                                    android:completionThreshold="0"
                                    android:imeOptions="actionNext"
                                    android:inputType="textNoSuggestions"
                                    android:textColor="@color/osl_ui_attribute_value_text_color"
                                    android:textSize="14sp"
                                    tools:ignore="TouchTargetSizeCheck" />
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/osl_rate_outer_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="15dp"
            android:background="@drawable/rounded"
            android:backgroundTint="#9E232323"
            android:orientation="vertical"
            android:padding="5dp"
            android:paddingHorizontal="15dp"
            android:paddingTop="10dp">

            <LinearLayout
                android:id="@+id/osl_rate_inner_container"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/rounded"
                android:backgroundTint="#9E232323"
                android:gravity="right"
                android:orientation="vertical"
                android:paddingLeft="20dp"
                android:paddingBottom="10dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView49"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/bai_jamjuree_medium"
                        android:text="Rate"
                        android:textColor="#C998958E"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                    <View
                        android:id="@+id/view4"
                        android:layout_width="wrap_content"
                        android:layout_height="2dp"
                        android:layout_weight="1" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="bottom"
                    android:gravity="bottom"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="top"
                        android:layout_weight="1"
                        android:foregroundGravity="top"
                        android:gravity="top"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView35"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/bai_jamjuree_medium"
                            android:gravity="top"
                            android:text="Delivery"
                            android:textColor="@color/osl_ui_attribute_label_text_color"
                            android:textSize="15sp" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="bottom"
                            android:orientation="vertical">

                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/one_shot_load_farm_rate"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="#00FFFFFF"
                                android:hint="₹ 0"
                                android:inputType="number"
                                android:paddingLeft="10dp"
                                android:paddingTop="10dp"
                                android:textColor="@color/osl_ui_attribute_value_text_color"
                                android:textColorHint="#3A3939"
                                android:textSize="17sp"
                                android:textStyle="bold" />
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView52"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/bai_jamjuree_medium"
                            android:gravity="bottom"
                            android:text="Farm"
                            android:textColor="@color/osl_ui_attribute_label_text_color"
                            android:textSize="15sp" />

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/osl_final_farm_rate"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="#00FFFFFF"
                            android:hint="₹ 0"
                            android:inputType="numberSigned|number"
                            android:paddingLeft="10dp"
                            android:paddingTop="10dp"
                            android:textColor="@color/osl_ui_attribute_value_text_color"
                            android:textColorHint="#3A3939"
                            android:textSize="24sp"
                            android:textStyle="bold" />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="left"
                android:orientation="horizontal"
                android:paddingVertical="7dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="20dp"
                    android:text="InHand Cash"
                    android:textColor="@color/osl_ui_attribute_label_text_color"
                    android:textSize="14sp" />

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/one_shot_load_extra_expense_provided"
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:background="#00FFFFFF"
                    android:gravity="center"
                    android:hint="₹ 0"
                    android:inputType="number"
                    android:textColor="#BD7805"
                    android:textSize="14sp" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <androidx.appcompat.widget.SwitchCompat
                android:id="@+id/osl_readiness_for_customer_communication"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/rounded"
                android:padding="10dp"
                android:paddingStart="20dp"
                android:text="Ready to send message to customer"
                android:textColor="#B1FFFFFF"
                android:visibility="visible"
                tools:visibility="visible" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="15dp"
            android:background="@drawable/rounded"
            android:backgroundTint="#9E232323"
            android:orientation="vertical"
            android:paddingHorizontal="15dp"
            android:paddingTop="5dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textView76"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:fontFamily="@font/bai_jamjuree_medium"
                        android:text="Salary"
                        android:textColor="#C998958E"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:foregroundGravity="right"
                    android:gravity="right"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="end"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <TextView
                            android:id="@+id/textView78"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right"
                            android:gravity="right"
                            android:text="Load: "
                            android:textColor="@color/osl_ui_attribute_label_text_color" />

                        <TextView
                            android:id="@+id/osl_estimated_load_kg"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right|end"
                            android:gravity="right|end"
                            android:text="000.000 kg"
                            android:textColor="@color/osl_ui_attribute_value_text_color" />
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:paddingTop="15dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/dashboard_reset_status_indicator"
                            android:layout_width="30dp"
                            android:layout_height="50dp"
                            android:background="@drawable/rounded"
                            android:backgroundTint="#54000000"
                            android:gravity="center"
                            android:onClick="clickDecreasePay"
                            android:text="-"
                            android:textColor="#555252"
                            android:textSize="24sp"
                            android:textStyle="bold" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="bottom|center_horizontal"
                            android:orientation="vertical">

                            <androidx.appcompat.widget.AppCompatImageView
                                android:id="@+id/imageView2"
                                android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:layout_weight="1"
                                android:background="@drawable/round_outline_for_avatar"
                                android:padding="2dp"
                                app:srcCompat="@drawable/profile_avatar"
                                app:tint="@color/osl_person_avatar_active" />

                            <LinearLayout
                                android:id="@+id/osl_driver_pay_container"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/osl_driver_base_pay"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:text="350"
                                    android:textColor="@color/osl_ui_attribute_label_text_color"
                                    android:textSize="12sp" />

                                <LinearLayout
                                    android:id="@+id/osl_driver_extra_pay_container"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/osl_driver_extra_pay"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="bottom|center"
                                        android:text="+0"
                                        android:textColor="#C8C5C5" />

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="1dp"
                                        android:layout_marginHorizontal="20dp"
                                        android:layout_marginVertical="3dp"
                                        android:layout_weight="1"
                                        android:background="#9A9797"
                                        android:orientation="vertical">

                                    </LinearLayout>

                                    <TextView
                                        android:id="@+id/osl_driver_total_pay"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:fontFamily="@font/biorhyme"
                                        android:gravity="center"
                                        android:text="total"
                                        android:textColor="@color/osl_ui_attribute_value_text_color" />

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="bottom|center_horizontal"
                                android:orientation="vertical"
                                android:paddingTop="10dp">

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:id="@+id/imageView9"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:layout_gravity="center_horizontal"
                                    android:background="@drawable/round_outline_for_avatar"
                                    android:padding="2dp"
                                    app:srcCompat="@drawable/profile_avatar"
                                    app:tint="@color/osl_person_avatar_active" />

                                <LinearLayout
                                    android:id="@+id/osl_labour1_pay_container"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/osl_labour1_base_pay"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:text="300"
                                        android:textColor="@color/osl_ui_attribute_label_text_color"
                                        android:textSize="12sp" />

                                    <LinearLayout
                                        android:id="@+id/osl_labour1_extra_pay_container"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:orientation="vertical">

                                        <TextView
                                            android:id="@+id/osl_labour1_extra_pay"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:gravity="bottom|center_horizontal"
                                            android:text="+0"
                                            android:textColor="#C8C5C5" />

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="1dp"
                                            android:layout_marginHorizontal="20dp"
                                            android:layout_marginVertical="3dp"
                                            android:layout_weight="1"
                                            android:background="#9A9797"
                                            android:orientation="vertical">

                                        </LinearLayout>

                                        <TextView
                                            android:id="@+id/osl_labour1_total_pay"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:fontFamily="@font/biorhyme"
                                            android:gravity="center"
                                            android:text="total"
                                            android:textColor="@color/osl_ui_attribute_value_text_color" />

                                    </LinearLayout>

                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="bottom|center_horizontal"
                                android:orientation="vertical"
                                android:paddingTop="10dp">

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:id="@+id/addLabour2"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:onClick="onClickAddLabour2"
                                    android:padding="2dp"
                                    app:srcCompat="@drawable/profile_avatar"
                                    app:tint="#9C514D4D" />

                                <LinearLayout
                                    android:id="@+id/osl_labour2_pay_container"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/osl_labour2_base_pay"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:gravity="bottom|center_horizontal"
                                        android:text="300"
                                        android:textColor="@color/osl_ui_attribute_label_text_color"
                                        android:textSize="12sp" />

                                    <LinearLayout
                                        android:id="@+id/osl_labour2_extra_pay_container"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:orientation="vertical">

                                        <TextView
                                            android:id="@+id/osl_labour2_extra_pay"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:gravity="bottom|center_horizontal"
                                            android:text="+0"
                                            android:textColor="#C8C5C5" />

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="1dp"
                                            android:layout_marginHorizontal="20dp"
                                            android:layout_marginVertical="3dp"
                                            android:background="#9A9797"
                                            android:orientation="vertical">

                                        </LinearLayout>

                                        <TextView
                                            android:id="@+id/osl_labour2_total_pay"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:fontFamily="@font/biorhyme"
                                            android:gravity="center"
                                            android:text="total"
                                            android:textColor="@color/osl_ui_attribute_value_text_color" />
                                    </LinearLayout>

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView73"
                            android:layout_width="30dp"
                            android:layout_height="50dp"
                            android:background="@drawable/rounded"
                            android:backgroundTint="#54000000"
                            android:backgroundTintMode="src_in"
                            android:gravity="center"
                            android:onClick="clickIncreasePay"
                            android:text="+"
                            android:textColor="#555252"
                            android:textSize="24sp" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/osl_total_salary"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/biorhyme"
                        android:gravity="center"
                        android:padding="20dp"
                        android:text="Total Salary"
                        android:textColor="#C88005"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight=".2"
                android:background="@drawable/rounded"
                android:backgroundTint="#86434545"
                android:hapticFeedbackEnabled="true"
                android:onClick="onClickOneShotLoadRefreshBtn"
                android:padding="4dp"
                android:tint="#7A0A0A0B"
                app:srcCompat="@android:drawable/ic_popup_sync" />

            <Button
                android:id="@+id/osl_save_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="10dp"
                android:layout_weight=".6"
                android:background="@drawable/round_outline_for_avatar"
                android:hapticFeedbackEnabled="true"
                android:onClick="onClickOneShotLoadSaveBtn"
                android:paddingTop="2dp"
                android:paddingBottom="0dp"
                android:text="Save" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight=".2"
                android:background="@drawable/rounded"
                android:backgroundTint="#56D32F2F"
                android:hapticFeedbackEnabled="true"
                android:onClick="onClickExitButton"
                android:padding="4dp"
                app:srcCompat="@android:drawable/ic_menu_close_clear_cancel"
                app:tint="#79F4EFEF" />

        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>